{
	"name": "@forgepoint/astro-integration-issues",
	"version": "0.1.0",
	"type": "module",
	"packageManager": "bun@1.1.30",
	"main": "./src/index.ts",
	"exports": {
		".": "./src/index.ts"
	},
	"scripts": {
		"build": "echo 'Integration package is consumed as source (no build needed)'",
		"typecheck": "tsc --noEmit",
		"codegen": "graphql-codegen -c codegen.ts",
